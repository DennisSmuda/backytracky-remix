{
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "run-s \"build:*\"",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run-p \"dev:*\"",
    "dev:css": "postcss ./styles/**/*.css --dir ./app/styles --watch",
    "dev:remix": "remix dev",
    "generate:css": "postcss ./styles/**/*.css --dir ./app/styles --env production",
    "test:build": "cross-env NODE_ENV=development remix build",
    "test:e2e": "cypress open",
    "test:unit": "vitest",
    "test:coverage": "vitest --coverage",
    "test:start": "cross-env NODE_ENV=production remix-serve ./build",
    "start:netlify": "cross-env NODE_ENV=production netlify dev",
    "typecheck": "tsc -b && tsc -b cypress",
    "tsc": "tsc -b",
    "prepare": "husky install",
    "postinstall": "npm run generate:css",
    "prebuild": "npm run generate:css"
  },
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.6",
    "@netlify/functions": "^1.0.0",
    "@prisma/client": "^4.1.0",
    "@remix-run/netlify": "1.19.3",
    "@remix-run/node": "2.1.0",
    "@remix-run/react": "2.1.0",
    "@tonaljs/tonal": "^4.6.5",
    "bcryptjs": "^2.4.3",
    "clsx": "^1.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.3.0",
    "remix-utils": "^7.1.0",
    "tone": "^14.7.77"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.3.0",
    "@remix-run/dev": "2.1.0",
    "@remix-run/eslint-config": "2.1.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.4",
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "@vitejs/plugin-react": "^4.1.0",
    "@vitest/coverage-v8": "^0.34.6",
    "autoprefixer": "^10.4.7",
    "cross-env": "^7.0.3",
    "cypress": "^13.3.2",
    "esbuild-register": "^3.3.3",
    "eslint": "^8.15.0",
    "eslint-plugin-testing-library": "^6.1.0",
    "happy-dom": "^12.9.1",
    "husky": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-cli": "^10.0.0",
    "postcss-import": "^14.1.0",
    "prisma": "^4.1.0",
    "tailwindcss": "^3.1.6",
    "typescript": "^5.1.0",
    "vite": "^4.5.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=14"
  }
}
